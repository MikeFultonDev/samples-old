ds=`(echo $1 | tr '[:lower:]' '[:upper:]')`
opercmd 'd prog,lnklst,name=current' | grep -q $ds
rc=$?
if [ $rc -gt 0 ]; then
	echo "Dataset: ${ds} is not in the current linklist"
	exit $rc
fi
opercmd 'setprog lnklst,undefine,name=tmp' | awk '/CSV514I/ { print "Temporary linklist still active. Activate linklist to apply address spaces to, then Use setprog lnklst,update,asid=<asid> to update address spaces"; exit(8); }' 
if [ $? -gt 0 ]; then
	exit 8
fi
opercmd 'setprog lnklst,define,name=tmp,copyfrom=current'  >/dev/null 2>&1
opercmd "setprog lnklst,delete,name=tmp,dsname=$ds"
opercmd 'setprog lnklst,activate,name=tmp' >/dev/null 2>&1

