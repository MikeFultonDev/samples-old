#
# dalld: Display (print) -all- datasets on all volumes, sorted
# This can take awhile to run. This could print out a -lot- of data
#
vols=`dvol | tail +5 | awk ' {print substr($0,68,6)}'`
dall=''
for v in $vols; do
	echo "Reading volume: ${v}"
	ds=`dvtoc $v | awk ' { print $1 } '`
	dall="${dall} ${ds}"
done
echo "${dall}" | sort